{
  "name": "nicespace-fe",
  "version": "1.0.0",
  "repository": "git@gitlab.com:thinkgroupv2/nicespace-fe.git",
  "author": "Vu Thanh Bay <vuthanhbayit@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "nicespace",
    "packages/*"
  ],
  "lint-staged": {
    "*.{ts,js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "nicespace:dev:desktop": "yarn workspace nicespace dev:desktop",
    "nicespace:dev:mobile": "yarn workspace nicespace dev:mobile",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "test": "vitest"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxtjs/eslint-config-typescript": "^9.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.8",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "vitest": "^0.9.3"
  },
  "engines": {
    "node": "^14.16.0"
  }
}
